[server]
host-name={{ system_hostname }}
domain-name=local
use-ipv4=yes
use-ipv6=no
allow-interfaces={{ wireless_interface }},{{ ethernet_interface }}
deny-interfaces=wg0
ratelimit-interval-usec=1000000
ratelimit-burst=1000

[wide-area]
enable-wide-area=yes

[publish]
publish-addresses=yes
publish-hinfo=yes
publish-workstation=yes
publish-domain=yes

[reflector]
enable-reflector=no

[rlimits]