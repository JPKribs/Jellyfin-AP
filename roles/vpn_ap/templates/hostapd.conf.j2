interface={{ wireless_interface }}
driver=nl80211
ssid={{ ap_ssid }}

hw_mode=g
channel={{ ap_channel }}
country_code=US

ieee80211n=1
wmm_enabled=1

ht_capab=[HT40+][SHORT-GI-20][SHORT-GI-40][MAX-AMSDU-7935]

macaddr_acl=0
auth_algs=1
ignore_broadcast_ssid=0

wpa=2
wpa_passphrase={{ ap_password }}
wpa_key_mgmt=WPA-PSK
rsn_pairwise=CCMP
ieee80211d=1
